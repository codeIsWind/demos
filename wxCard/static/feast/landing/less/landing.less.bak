@import "./normalize";
// Variables
@images: "../images";
html,body{
	height:100%;
}
#container {
    	box-sizing: border-box;
	width: 100%;
	height:33.35rem;
	background:url("@{images}/background@2x.png") no-repeat center;
	background-size:100% 100%;
	padding-top:1.5rem;
}
.logo{
	width:2.9rem;
	margin:0 auto 0.1rem;
	font-size:0px;
	img{
		width:100%;
	}
}
.info{
	width:12.55rem;
	margin:0 auto 1.05rem;
	font-size:0px;
	img{
		width:100%;
	}
}
.txt{
	border-top:1px solid #d8d8d8;
	width:8.8rem;
	height:0px;
	margin:0 auto;
	text-align:center;
	span{
		color:#9b9b9b;
		font-size:0.7rem;
		display:inline-block;
		background:#fff;
		padding:0 0.5rem;
		height:0.7rem;
		position:relative;
		top:-0.7rem;
	}
}
a{
	-webkit-tap-highlight-color: rgba(0,0,0,0);
}
.load-link{
	background:url("@{images}/Rectangle@2x.png") no-repeat 0 0;
	background-size:100% 100%;
	color:#fff;
	font-size:0.75rem;
	width:9.5rem;
	text-align:center;
	height:2.45rem;
	line-height:2.45rem;
	display:block;
	margin:1.6rem auto 0.9rem;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	-webkit-touch-callout: none;
	text-decoration: none;
}
.load-link:hover{
	background:url("@{images}/Rectangle_pressed@2x.png") no-repeat 0 0;
	background-size:100% 100%;
}
.load-link:active{
	background:url("@{images}/Rectangle_pressed@2x.png") no-repeat 0 0;
	background-size:100% 100%;
}
.load-link-on{
	background:url("@{images}/Rectangle_pressed@2x.png") no-repeat 0 0;
	background-size:100% 100%;
}
.phone{
	width:12.35rem;
	margin:0 auto;
	font-size:0px;
	img{
		width:100%;
	}
}
